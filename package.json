{
  "name": "redux-multipurpose-toolkit",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "redux-fix": "node ./scripts/redux-missing-symbol-observable-fix.js",
    "redux-persist-fix": "node ./scripts/redux-persist-react-fix.js",
    "prebuild": "npm run redux-fix && npm run redux-persist-fix",
    "build": "ng build @redux-multipurpose/core && ng build @redux-multipurpose/angular-router",
    "build-watch": "ng build @redux-multipurpose/core --watch",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "unlink": "cd dist/redux-multipurpose/core && npm unlink && cd ../angular-router && npm unlink && cd ../../..",
    "clean": "npm run unlink && rm -rf dist/ node_modules/ package-lock.json",
    "link": "cd dist/redux-multipurpose/core && npm link && cd ../angular-router && npm link &&  cd ../../..",
    "pack": "cd dist/redux-multipurpose/core && npm pack && cd ../angular-router && npm pack && cd ../../..",
    "build-link": "npm run build && npm run link",
    "build-pack": "npm run build && npm run pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "@reduxjs/toolkit": "^1.4.0",
    "flux-standard-action": "^2.1.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-observable-es6-compat": "^1.2.1",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-encrypt": "^2.0.1",
    "redux-saga": "^1.1.3",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "^10.0.4",
    "@angular/compiler": "~10.0.5",
    "@angular/compiler-cli": "~10.0.5",
    "@angular/language-service": "~10.0.5",
    "@types/jasmine": "^3.5.11",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.0.27",
    "codelyzer": "^6.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.3.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "ng-packagr": "^10.0.3",
    "protractor": "^7.0.0",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "~3.9.7"
  }
}
