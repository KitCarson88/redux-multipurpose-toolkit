{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@redux-multipurpose/core": [
        "dist/redux-multipurpose/core/redux-multipurpose-core",
        "dist/redux-multipurpose/core"
      ],
      "@redux-multipurpose/saga": [
        "dist/redux-multipurpose/saga/redux-multipurpose-saga",
        "dist/redux-multipurpose/saga"
      ],
      "@redux-multipurpose/toolkit": [
        "dist/redux-multipurpose/toolkit/redux-multipurpose-toolkit",
        "dist/redux-multipurpose/toolkit"
      ],
      "@redux-multipurpose/saga-epic": [
        "dist/redux-multipurpose/saga-epic/redux-multipurpose-saga-epic",
        "dist/redux-multipurpose/saga-epic"
      ],
      "@redux-multipurpose/persist": [
        "dist/redux-multipurpose/persist/redux-multipurpose-persist",
        "dist/redux-multipurpose/persist"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}